<block wx:if="{{$taroCompReady}}">
    <view class="InputBar">
        <view class="InputBar-top">
            <view class="view-image">
                <image class="image" bindtap="anonymousFunc0" mode="widthFix" src="https://oss.globalcharge.cn/prod/wechat/countryFlags/China.svg"></image>
            </view>
            <view class="phoneInputBar">
                <view class="areaNumber">+86</view>
                <view class="phoneInput">
                    <at-input compid="{{$compid__230}}"></at-input>
                </view>
            </view>
        </view>
        <view class="{{(state.phoneInputHighLight && 'active') + ' phoneInputBg'}}"></view>
        <view class="InputBar-bottom">
            <view class="countryName">中国</view>
            <view class="carrierName">中国移动</view>
        </view>
        <country-list compid="{{$compid__231}}"></country-list>
    </view>
</block>