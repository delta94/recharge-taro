.phoneList {
  top: 140rpx;
  position: absolute;
  width: 100%;
  left: 0;
  background-color: #fff;
  box-sizing: border-box;
  display: none;
  padding: 0 24rpx 24rpx;
}
.phoneList.active {
  display: block;
}
.phoneList-item {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #2A2A2A;
  padding: 20rpx 0;
  border-bottom: 1PX solid #F2F2F2;
  font-weight: bold;
}
.phoneList-item .areaNumber {
  padding: 0 .7em 0 .5em;
}
.phoneList-item .phone {
  flex: 1;
}
.phoneList-item .name {
  color: #888888;
  font-size: 24rpx;
  font-weight: normal;
}