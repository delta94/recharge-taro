.view-image .image {
  width: 100%;
  height: 94rpx;
}
.carrierList-item {
  display: flex;
  align-items: center;
}
.carrierList-item .view-image {
  width: 94rpx;
  margin-right: 26rpx;
}
.carrierList-item .name {
  flex: 1;
}
.carrierList-item .check {
  width: 52rpx;
